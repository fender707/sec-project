<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>Wisten OnePage HTML Theme</title>
  <meta name="description" content="" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <!-- CSS Files -->
  <%= stylesheet_link_tag "css/reset.css" %>
  <%= stylesheet_link_tag "css/animate.min.css" %>
  <%= stylesheet_link_tag "css/bootstrap.css" %>
  <%= stylesheet_link_tag "css/style.css" %>
  <%= stylesheet_link_tag "css/flexslider.css" %>
  <%= stylesheet_link_tag "css/font-awesome.css" %>
  <%= stylesheet_link_tag "css/owl.carousel.css" %>
  <%= stylesheet_link_tag "css/settings.css" %>
  <%= stylesheet_link_tag "css/prettyPhoto.css" %>
  <%= stylesheet_link_tag "css/responsive.css" %>
  <%= stylesheet_link_tag "css/player/YTPlayer.css" %>

  <!-- End CSS Files -->
  <!-- JS Files -->

  <%= javascript_include_tag "js/jquery-1.10.2.min.js" %>
  <%= javascript_include_tag "js/bootstrap.js" %>
  <%= javascript_include_tag "js/jquery.appear.js" %>
  <%= javascript_include_tag "js/waypoints.min.js" %>
  <%= javascript_include_tag "js/jquery.prettyPhoto.js" %>
  <%= javascript_include_tag "js/modernizr-latest.js" %>
  <%= javascript_include_tag "js/SmoothScroll.js" %>
  <%= javascript_include_tag "js/jquery.parallax-1.1.3.js" %>
  <%= javascript_include_tag "js/jquery.easing.1.3.js" %>
  <%= javascript_include_tag "js/jquery.flexslider.js" %>
  <%= javascript_include_tag "js/jquery.sticky.js" %>
  <%= javascript_include_tag "js/owl.carousel.js" %>
  <%= javascript_include_tag "js/jquery.isotope.js" %>
  <%= javascript_include_tag "js/rev-slider/jquery.themepunch.plugins.min.js" %>
  <%= javascript_include_tag "js/rev-slider/jquery.themepunch.revolution.min.js" %>
  <%= javascript_include_tag "js/jquery.mb.YTPlayer.js" %>

  <!-- End JS Files -->

  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>

</body>
</html>
